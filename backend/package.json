{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon",
    "start:reseed": "cd db/ && psql -f seed.sql && cd .. && npm run start:dev",
    "start:reseed-pg12": "cd db/ && /Applications/Postgres.app/Contents/Versions/12/bin/psql -p5433 -f seed.sql && cd .. && npm run start:dev",
    "start:resetDB": "heroku pg:reset DATABASE",
    "start:reseedHeroku": "npm run start:resetDB && heroku pg:psql -f db/seed.sql"
  },
  "lint-staged": {
    "*.{js,json}": [
      "npx eslint . --fix"
    ],
    "**/*.{js.json}": [
      "npx eslint . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "firebase-admin": "^11.4.1",
    "icecat": "^1.4.5",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.5.0",
    "socket.io": "^2.3.0",
    "tesseract.js": "^2.1.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
