{
  "name": "pantry",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clearmods": "rm -rf node_modules/ && npm i && npm install --only=dev && cd ios/ && pod install --repo-update && cd ..",
    "test": "jest",
    "lint": "npx eslint . --ext js,ts,jsx,ts,json --fix",
    "format": "npx prettier --write src/**/*.{js,ts,tsx} && npx prettier --write *.{js,json}",
    "start:android": "npm run start:debugger && npx react-native run-android",
    "start:app": "cd ios/ && pod install --repo-update && cd .. && npm run start:debugger && npx react-native run-ios",
    "start:mike": "npm run start:debugger && npx react-native run-ios --device='Michael D.â€™s iPhone'",
    "start:debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "npx prettier --write",
      "npx eslint --fix"
    ],
    "*.{js,json}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^2.3.2",
    "@react-native-community/masked-view": "^0.1.9",
    "@react-native-community/picker": "^1.4.0",
    "@react-native-firebase/app": "^6.4.0",
    "@react-native-firebase/auth": "^6.4.0",
    "@react-native-firebase/ml-vision": "^6.4.0",
    "@react-native-firebase/storage": "^6.4.0",
    "@react-navigation/bottom-tabs": "^5.2.5",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "@types/react-native-check-box": "^2.1.1",
    "axios": "^0.19.2",
    "babel-plugin-root-import": "^6.5.0",
    "firebase": "^7.14.0",
    "fs": "0.0.1-security",
    "metro-config": "^0.59.0",
    "native-base": "^2.13.12",
    "neumorphism-ui": "^1.0.2",
    "react": "16.11.0",
    "react-devtools": "^4.6.0",
    "react-native": "0.62.1",
    "react-native-camera": "^3.23.1",
    "react-native-check-box": "^2.1.7",
    "react-native-cli": "^2.0.1",
    "react-native-date-picker": "^2.7.9",
    "react-native-datepicker": "^1.7.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-draggable-flatlist": "^2.3.3",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-share": "^3.3.0",
    "react-native-simple-toast": "^1.1.2",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swipe-list-view": "^3.1.0",
    "react-native-text-detector": "0.0.6",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.3.7",
    "react-navigation-stack": "^2.3.11",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.2",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.11.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
